{
  "rewrites": [
    {
      "source": "/share/gcs/:file",
      "destination": "https://api.plusc.kalinpatel.me/file/:file"
    },
    {
      "source": "/external/:url",
      "destination": ":url"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/link/:path",
      "destination": "https://api.plusc.kalinpatel.me/url/:path"
    }
  ],
  "github": {
    "silent": true
  }
}
